    "@nuxt/scripts": "^0.13.0",
    "@nuxt/test-utils": "^3.20.1",
    "@unhead/vue": "^2.0.19",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nuxt": "^4.2.0",
    "prettier": "^3.6.2",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  }
